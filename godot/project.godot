; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Gtv 2023"
run/main_scene="res://src/world/example.tscn"
config/features=PackedStringArray("4.0", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

RngUtils="*res://addons/rng_utils/rng_utils.gd"
Logger="*res://addons/gs_logger/logger.gd"
SceneManager="*res://addons/scene_manager/SceneManager.tscn"
Globals="*res://src/globals.tscn"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/initial_position_type=2
window/size/resizable=false

[editor]

script/templates_search_path="res://addons/xsm/templates/"

[editor_plugins]

enabled=PackedStringArray("res://addons/rng_utils/plugin.cfg", "res://addons/gs_logger/plugin.cfg", "res://addons/scene_manager/plugin.cfg", "res://addons/xsm/plugin.cfg")

[input]

dash={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":4194325,"key_label":0,"unicode":0,"echo":false,"script":null)
]
}
jump={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":32,"key_label":0,"unicode":32,"echo":false,"script":null)
]
}

[layer_names]

2d_physics/layer_1="world"
2d_physics/layer_5="player"
2d_physics/layer_6="enemy"
2d_physics/layer_7="projectiles"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
